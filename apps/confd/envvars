# env file for confd
        
CONFD_HOME=/var/lib/apps/confd      
CONFD_CMD=/var/lib/apps/confd/bin/confd
CONFD_CMD_REPO=https://github.com/kelseyhightower/confd/releases/download/v0.5.0/confd-0.5.0-linux-amd64
CONFD_OPTS="-verbose=true \
        -interval=10 \
        -node='127.0.0.1:4001' \
        -confdir=/var/lib/apps/confd"
