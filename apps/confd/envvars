# env file for confd
        
CONFD_HOME=/var/lib/apps/confd      
CONFD_CMD=/var/lib/apps/confd/bin/confd
CONFD_DST=/var/lib/apps-data/haproxy
CONFD_CMD_REPO=https://github.com/kelseyhightower/confd/releases/download/v0.11.0/confd-0.11.0-linux-amd64
CONFD_OPTS=-interval=10 -node=127.0.0.1:2379 -confdir=/var/lib/apps/confd
